#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    
    int t;
    cin>>t;
    
    
    while(t--)
    
    {
        int n;
    cin>>n;
    
    map<int,int> m1;
    int max1=0;
    for(int i=0;i<n;++i)
    {
        int c;
        cin>>c;
        
        m1[c]++;
        
        max1=max(max1,m1[c]);
    }
    
    // if(max1==1)
    // {
    //     cout<<0<<endl;
    // }
    // else if(max1==2)
    // {
    //     cout<<1<<endl;
    // }
    // else if(max1>2)
    // {
    //     cout<<max1-1<<endl;
    // }
    int c=0;
    while(max1>1)
    {
        if(max1%2!=0)
        {
            max1=(max1/2)+1;
        }
        else
        max1=max1/2;
        
        c++;
    }
    
    cout<<c<<endl;
        
    }
    
    
    
	// your code goes here
	return 0;
}
